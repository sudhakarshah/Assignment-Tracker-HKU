{
  "manifest_version": 2,

  "name": "Assignment Reminder",
  "description": "Keeps a track of assignments for students at the University of Hong Kong ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
    /*"default_popup": "popup.html"*/
  },
  "background":{
    "scripts":["background.js"]
  },
  "content_scripts":[
  { "run_at": "document_end",
    "matches":["https://moodle.hku.hk/mod/assign/view.php?*","http://moodle.hku.hk/mod/assign/view.php?*"],
    "js":["myScript.js"]
  }
  /*,
 {
    "matches":["http://moodle.hku.hk/mod/turnitintooltwo/*","https://moodle.hku.hk/mod/turnitintooltwo/*","file:///Users/sudhakarshah/Downloads/not%20submitted%20turnitin%20source%20code.htm","file:///Users/sudhakarshah/Downloads/Submitted%20turnitin.htm"],
    "js":["turnitinMyScript.js"]
  }*/
  ,

  {
    "matches": [
            "https://*/*",
            "http://*/*"
        ],
    "js": ["content-script.js"]
  }

],

"web_accessible_resources": ["popup.html"],

"chrome_url_overrides":{

},
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
